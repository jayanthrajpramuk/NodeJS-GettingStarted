{
  "name": "library",
  "description": "Keeping track of NODEJS components via Git",
  "main": "app.js",
  "authors": [
    "Jayanth C S"
  ],
  "license": "ISC",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "select2": "~4.0",
    "moment":"2.20.1",
    "moment-timezone":"0.5.14",
    "eonasdan-bootstrap-datetimepicker":"^4.17.0"
  },
  "overrides": {
    "bootstrap":{
      "main" : [
            "dist/js/bootstrap.js",
            "dist/css/bootstrap.css"
      ]
    },
    "font-awesome":{
      "main" : [
        "less/font-awesome.less",
        "scss/font-awesome.scss",
        "css/font-awesome.min.css"
      ]
    },
    "select2":{
      "main" : [
              "dist/css/select2.css",
              "dist/js/select2.js"
      ]
   },
    "moment":{
      "main":[
        "src/moment.js",
        "locale/*.js",
        "min/moment-with-locales.js"
      ]
    },
     "moment-timezone":{
      "main":[
        "builds/moment-timezone.min.js"
      ]
    },
    "eonasdan-bootstrap-datetimepicker":{
      "main":[
        "src/js/bootstrap-datetimepicker.js",
        "src/less/_bootstrap-datetimepicker.less",
        "src/scss/_bootstrap-datetimepicker.scss"
      ]
    }
  }
}
